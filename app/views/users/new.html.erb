<% javascript 'new-user' %>
<%= title "Signup for #{membership_description(params[:mt]).downcase}", :h1  %>
<% form_for @user do |f| %>
  <%= render :partial => "form", :locals => {:f => f, :with_password => true, :button_msg => params[:mt]=="free_listing"?"Sign up":"Proceed to payment"  }   %>
<% end %>