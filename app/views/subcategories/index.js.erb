var subcategories = new Array(<%= @subcategories.size %>);
<% @subcategories.each_with_index do |subcategory, index| %>
sbg[<%= index %>] = "<%= escape_javascript(subcategory.name) %>";
<% end %>
<% @members.each_with_index do |member, index| -%>
sbg[<%= @subcategories.size + index%>]="<%= escape_javascript(member.full_name)%>";
<% end -%>
